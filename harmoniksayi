import java.util.Scanner;
public class Main {
    public static void main(String[] args) {

        int sayi;
        double harmseritop=0.0;

        Scanner inp = new Scanner(System.in);
        System.out.print("Harmonik dizisi bulunacak sayÄ±=");
        sayi=inp.nextInt();

        for (int i=1;i<=sayi;i++){
            harmseritop+=(1.0/i);
        }
        System.out.print("Toplam="+harmseritop);


    }
}
